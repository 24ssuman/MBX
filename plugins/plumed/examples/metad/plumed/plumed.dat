UNITS LENGTH=A ENERGY=kcal/mol TIME=fs

dclo: DISTANCE ATOMS=1,2
thw: ANGLE ATOMS=2,3,4
rmsd: DRMSD REFERENCE=cl-h2o.pdb LOWER_CUTOFF=1 UPPER_CUTOFF=50 TYPE=INTER-DRMSD

mtd:   METAD ARG=rmsd PACE=500 SIGMA=0.5 HEIGHT=0.8 FILE=plumed/HILLS BIASFACTOR=6 TEMP=800
uwall: UPPER_WALLS ARG=dclo AT=6.5 KAPPA=250

PRINT ARG=dclo,thw,rmsd,mtd.*,uwall.* STRIDE=10 FILE=plumed/COLVAR
FLUSH STRIDE=1
